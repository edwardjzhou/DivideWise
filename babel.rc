
{
  "presets": [
    "es2015",
    "env",
    "react",
    "stage-2",
    "stage-0",
    "@babel/preset-env",
    "@babel/preset-react"
  ]
},

{
  "plugins": [
    "transform-es2015-arrow-functions",
    "check-es2015-constants",
    "transform-es2015-block-scoping",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-object-rest-spread",
    "emotion"
  ]
} 
